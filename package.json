{
  "name": "jsonapi-server",
  "version": "4.2.0",
  "description": "A config driven NodeJS framework implementing json:api",
  "keywords": [
    "jsonapi",
    "json:api",
    "swagger",
    "api"
  ],
  "main": "lib/jsonApi.js",
  "author": "Oliver Rumbelow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/holidayextras/jsonapi-server"
  },
  "engines": {
    "node": ">=6"
  },
  "greenkeeper": {
    "ignore": [
      "express-graphql",
      "graphql",
      "joi",
      "swagger-tools"
    ]
  },
  "dependencies": {
    "async": "3.2.0",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "debug": "4.2.0",
    "express": "4.17.1",
    "express-graphql": "0.11.0",
    "graphql": "15.4.0",
    "joi": "17.3.0",
    "lodash.assign": "4.2.0",
    "lodash.isequal": "4.5.0",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "lodash.uniq": "4.5.0",
    "lodash.uniqby": "4.7.0",
    "qs": "6.9.4",
    "request": "2.88.0",
    "use-strict": "1.0.1",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "coveralls": "3.1.0",
    "eslint": "7.12.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-config-standard": "16.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.2",
    "jscpd": "3.3.19",
    "lokka": "1.7.0",
    "lokka-transport-http": "1.6.1",
    "mocha": "8.2.1",
    "mocha-performance": "0.1.1",
    "nyc": "15.1.0",
    "swagger-tools": "0.10.4"
  },
  "scripts": {
    "start": "node example/server.js",
    "test": "nyc mocha",
    "coverage": "[ -n \"$TRAVIS\" ] && nyc report --reporter=text-lcov | coveralls || nyc report --reporter=html",
    "performance": "node --allow-natives-syntax --harmony ./node_modules/mocha/bin/_mocha -S --reporter mocha-performance ./test/*.js",
    "lint": "eslint ./example ./lib ./test --quiet && echo 'âœ” All good!'",
    "jscpd": "jscpd --blame -p ./lib/ || echo 'Finished!'",
    "swagger": "mocha ./swaggerValidator.js --timeout 60000",
    "ci": "npm run jscpd && npm run lint && npm run test && npm run swagger"
  }
}
